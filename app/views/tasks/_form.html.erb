<%= form_for(@task) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :completed,'Is an admin account?', class:"form-check-label" %>
  <%= f.check_box :completed,{class:"form-check-input", type:"checkbox"}, true, false %>
  <%= f.submit %>
<% end %>
